.container {
    display: flex;
    width: 100%;
    flex-direction: column;
    align-items: center;
}

.header {
    display: flex;
    width: 100%;
    height: 100rpx;
    padding: 0 20rpx;
    justify-content: space-between;
    align-items: center;
    box-sizing: border-box;
    border-top: 1px solid #eee;
    border-bottom: 1px solid #eee;
}

.cpt-wrap {
    width: 100%;
}

.nav-bar {
    position: absolute;
    bottom: 40rpx;
}
